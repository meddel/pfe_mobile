<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.gest_map.GestMapFragment">

    <ImageView
        android:id="@+id/imageV4"
        android:layout_width="197dp"
        android:layout_height="159dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/ic_menu_station"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="bus_dialog"/>

    <ImageView
        android:id="@+id/imageV2"
        android:layout_width="144dp"
        android:layout_height="140dp"
        android:layout_marginTop="64dp"
        android:onClick="openDialog"
        android:src="@drawable/ic_menu_map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageV4"
        app:layout_constraintVertical_bias="0.097" />

</androidx.constraintlayout.widget.ConstraintLayout>